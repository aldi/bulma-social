{
  "name": "bulma-social",
  "description": "Social Buttons and Colors for Bulma",
  "author": {
    "name": "aldi",
    "url": "https://github.com/aldi"
  },
  "version": "3.0.1",
  "homepage": "https://aldi.github.io/bulma-social",
  "jsdelivr": "css/all.min.css",
  "unpkg": "css/all.min.css",
  "style": "css/all.min.css",
  "exports": {
    ".": "./css/all.min.css",
    "./css/*": "./css/*",
    "./sass/*": "./sass/*"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "keywords": [
    "bulma",
    "social",
    "bulma-social",
    "social-buttons",
    "css"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/aldi/bulma-social.git"
  },
  "bugs": {
    "url": "https://github.com/aldi/bulma-social/issues"
  },
  "peerDependencies": {
    "bulma": ">=1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.24",
    "cssnano": "^7.0.6",
    "postcss": "^8.4.49",
    "rimraf": "^6.1.3",
    "sass": "^1.97.3"
  },
  "scripts": {
    "build": "npm run clean && node scripts/build.js",
    "clean": "rimraf css",
    "check:providers": "node scripts/check-provider-sync.js",
    "watch": "sass --watch sass/social-providers/_all.scss:css/all.css"
  },
  "files": [
    "css",
    "sass",
    "LICENSE",
    "README.md"
  ],
  "browserslist": [
    "baseline widely available with downstream"
  ]
}
